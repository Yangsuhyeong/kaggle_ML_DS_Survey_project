      "source": [
        "def to_onehot(i):\n",
        "    num = 'Q'+str(i)\n",
        "    q = multiple[[t for t in multiple.columns if num in t]]\n",
        "    q = q.drop([t for t in q.columns if 'OTHER_TEXT' in t],axis=1)  \n",
        "    q=q.fillna(0)\n",
        "    q_r = []\n",
        "    for col in q.columns:\n",
        "        if q[col].unique()[0]!=0:\n",
        "            s = q[col].unique()[0]\n",
        "            q_r.append(s)\n",
        "        else:\n",
        "            s = q[col].unique()[1]\n",
        "            q_r.append(s)\n",
        "        q[col]=q[col].replace(s,1)\n",
        "    q.columns=q_r\n",
        "    return q"
      ],
      "execution_count": null,
      "outputs": []
    },
