      "source": [
        "not_disclosed = multiple[multiple.value == 'I do not wish to disclose my approximate yearly compensation'].index\n",
        "multiple2 = pd.DataFrame()\n",
        "multiple2 = multiple.copy()\n",
        "multiple2['value'] = multiple['value'].replace(['I do not wish to disclose my approximate yearly compensation'], '0-0')"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
